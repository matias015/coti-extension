{
    "manifest_version":2,
    "name":"price calc",
    "description": "calcula precios",
    "version": "1.0",
    "icons":{
        "256":"icon.png"
    },
    "background":{
        "scripts":["background.js"]
    },

    "commands":{
        "start":{
            "suggested_key":{
                "default":"Ctrl+Shift+L",
                "mac":"Command+Shift+L"
            },
            "description": "start app",
            "shortcut":"Ctrl+Shift+L",
            "global": true
        }
    },

    "browser_action":{
        "default_icon":"icon.png"
    },
    "permissions":[
        "activeTab",
        "tabs",
        "http://www.cotilloncasaalberto.com.ar/*",
        "https://www.cotilloncasaalberto.com.ar/*",
        "tts",
        "storage"
    ]
}
